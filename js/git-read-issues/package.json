{
    "name": "@scramjet/git-read-issues",
    "version": "1.0.0",
    "description": "Sequence that reads github issues and store them in a topic prepared to use in clickup-add-issues",
    "main": "index.js",
    "scripts": {
        "build:refapps": "tsc -p tsconfig.json",
        "postbuild:refapps": "yarn packseq",
        "packseq": "PACKAGES_DIR=js node ../../scripts/packsequence.js",
        "clean": "rm -rf ./dist"
    },
    "keywords": [
        "github",
        "API",
        "issues"
    ],
    "author": "Scramjet <open-source@scramjet.org>",
    "license": "ISC",
    "dependencies": {
        "@scramjet/utility": "^0.38.0",
        "octokit": "^2.0.14",
        "scramjet": "^4.36.9"
    },
    "devDependencies": {
        "@octokit/types": "^9.0.0",
        "@scramjet/types": "^0.38.0",
        "@types/node": "^15.12.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/scramjetorg/transform-hub.git"
    }
}